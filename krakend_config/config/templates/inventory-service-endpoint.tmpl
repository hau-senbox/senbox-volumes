{
  "endpoint": "/user/",
  "input_query_strings":["*"],
  "method": "GET",
  "extra_config": {},
  "backend": [
    {
      "url_pattern": "/user/",
      "extra_config": {
        "qos/circuit-breaker": {
          "interval": 60,
          "timeout": 10,
          "max_errors": 1,
          "log_status_change": true
        }
      },
      "method": "GET",
      {{ if $sd_dns }}
