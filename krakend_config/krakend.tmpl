{
  "$schema": "https://www.krakend.io/schema/v3.json",
  "version": 3,
  "name": "Krakend Api Gateway",
  "timeout": "120000ms",
  "cache_ttl": "300s",
  "extra_config": {
    "telemetry/logging": {
      "level": "INFO",
      "prefix": "[KRAKEND]",
      "syslog": false,
      "stdout": true,
      "format": "default"
    },
    "security/cors": {
      "allow_origins": [
        "*"
      ],
      "allow_methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "OPTIONS",
        "PATCH"
      ],
      "expose_headers": [
        "Content-Length",
        "Content-Type",
        "Set-Cookie",
        "Authorization"
      ],
      "allow_headers": [
        "Accept-Language",
        "Content-Type",
        "Authorization",
        "Cookie",
        "X-Key",
        "X-Requested-With"
      ],
      "max_age": "12h",
      "allow_credentials": true,
      "debug": false
    },
    "auth/api-keys": {
      "strategy": "header",
      "identifier": "X-Key",
      "hash": "fnv128",
      "salt": "mySalt",
      "@hashed-with": "https://go.dev/play/p/hHksLLCUJy3",
      "keys": [
        {
          "@key-plain": "5enB@x",
          "key": "ae2ae7c06f03b2fcaebdd2b8b75b2a26",
          "roles": ["all"],
          "@description": "ACME Inc."
        }
      ]
    }
  },
  "endpoints": [
    {{ template "endpoint" .urls_dev }}
  ]
}
